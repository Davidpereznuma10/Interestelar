{
    "nombre": {
        "type": "string",
        "description": "User's name.",
        "example": "John Doe"
    },
    "discordUser": {
        "type": "string",
        "description": "Discord username.",
        "example": "johndoe#1234"
    },
    "email": {
        "type": "string",
        "description": "Email address.",
        "example": "johndoe@example.com"
    },
    "group": {
        "type": "string",
        "description": "User's group.",
        "example": "M1"
    },
    "entries": {
        "type": "array",
        "description": "User's activity entries.",
        "example": [
            {
                "date": "2023-09-25",
                "time": "15:30:00",
                "videos": [
                    {
                        "course": "react",
                        "name": "Video 1",
                        "url": "https://example.com/video1",
                        "lastWatchedTime": "2:50" 
                    },
                    {
                        "course": "node",
                        "name": "Video 2",
                        "url": "https://example.com/video2",
                        "lastWatchedTime": "0:00" 
                    }
                ]
            }
        ],
        "schema": [
            {
                "date": {
                    "type": "date",
                    "description": "Entry date.",
                    "example": "2023-09-25"
                },
                "time": {
                    "type": "time",
                    "description": "Entry time.",
                    "example": "15:30:00"
                },
                "videos": {
                    "type": "array",
                    "description": "Associated videos, if any.",
                    "example": [
                        {
                            "course": "react",
                            "name": "Video 1",
                            "url": "https://example.com/video1",
                            "lastWatchedTime": "2:50"
                        }
                    ],
                    "schema": [
                        {
                            "course": {
                                "type": "string",
                                "description": "Course name.",
                                "example": "React"
                            },
                            "name": {
                                "type": "string",
                                "description": "Video name.",
                                "example": "Introduction to MongoDB"
                            },
                            "url": {
                                "type": "string",
                                "description": "Video URL.",
                                "example": "https://example.com/video"
                            },
                            "lastWatchedTime": {
                                "type": "time",
                                "description": "Last watched time in the video.",
                                "example": "2:50"
                            }
                        }
                    ]
                }
            }
        ]
    }
}